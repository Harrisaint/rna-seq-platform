{
  "buildCommand": "cd web-new && npm ci && npm run build",
  "outputDirectory": "web-new/dist",
  "installCommand": "cd web-new && npm ci",
  "framework": "vite",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "web-new/src/api/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "VITE_API_URL": "@vite_api_url"
  }
}
